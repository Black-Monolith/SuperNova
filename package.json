{
  "name": "supernova",
  "version": "0.0.1",
  "description": "Electron + React + Redux + TypeScript + Webpack + TypeStyle Boilerplate",
  "main": "./app/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod",
    "dev": "concurrently \"webpack-dev-server --config ./webpack/targets/webpack.renderer\" \"webpack -w --config ./webpack/targets/webpack.main\""
