    "monolite": "^0.3.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "typestyle": "^0.26.1",
    "webpack-dev-server": "2.2.0",
    "when-switch": "^0.2.0"
  },
  "devDependencies": {
    "@types/electron": "^1.4.31",
    "@types/react": "^15.0.1",
    "@types/react-dom": "^0.14.21",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^2.2.2",
    "@types/webpack-env": "^1.13.0",
    "@types/webpack-merge": "^0.0.4",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.4",
    "csx": "^8.2.1",
    "electron": "^1.4.15",
    "html-webpack-plugin": "^2.28.0",
    "source-map-loader": "^0.1.6",
    "ts-loader": "^2.0.0",
    "typescript": "2.2.0-dev.20170127",
    "webpack": "2.2.0",
    "webpack-merge": "^2.5.0"
  }
}
