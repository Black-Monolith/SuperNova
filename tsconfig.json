{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "jsx": "react",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "src",
    "outDir": "dist"
  }
}
